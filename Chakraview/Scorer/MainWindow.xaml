<Window x:Class="Shenoy.Quiz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shenoy.Quiz"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="200">
    <StackPanel Orientation="Vertical">
        <Button Content="Prelims" Click="ShowPrelims" Width="120" Margin="10,10,0,0" />
        <Button Content="Finals" Click="ShowFinals" Width="120" Margin="10,10,0,0"  />
    </StackPanel>
</Window>
